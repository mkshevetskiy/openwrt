/dts-v1/;

/* Bootloader installs ATF here */
/memreserve/ 0x80000000 0x200000;

#include <dt-bindings/leds/common.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "an7581.dtsi"

/ {
	model = "Airoha AN7581 Evaluation Board";
	compatible = "airoha,an7581-evb", "airoha,an7581", "airoha,en7581";

	aliases {
		serial0 = &uart1;
	};

	chosen {
		bootargs = "console=ttyS0,115200 earlycon";
		stdout-path = "serial0:115200n8";
		linux,usable-memory-range = <0x0 0x80200000 0x0 0x1fe00000>;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x2 0x00000000>;
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		btn-0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&en7581_pinctrl 0 GPIO_ACTIVE_LOW>;
		};

		btn-1 {
			label = "wifi_2g";
			gpios = <&en7581_pinctrl 4 GPIO_ACTIVE_LOW>;
		};

		btn-2 {
			label = "wifi_5g";
			gpios = <&en7581_pinctrl 5 GPIO_ACTIVE_LOW>;
		};

		btn-3 {
			label = "led_enable";
			gpios = <&en7581_pinctrl 6 GPIO_ACTIVE_LOW>;
		};

		btn-4 {
			label = "wps";
			gpios = <&en7581_pinctrl 7 GPIO_ACTIVE_LOW>;
		};
	};

	pwmleds-0 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio16_idx8_pins>;

		pwm-0 {
			label = "wps:green";
			pwms = <&en7581_pwm 8 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};

	pwmleds-1 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio25_idx9_pins>;

		pwm-0 {
			label = "xPonLos:red";
			pwms = <&en7581_pwm 9 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};

	pwmleds-2 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio27_idx11_pins>;

		pwm-0 {
			label = "power:green";
			pwms = <&en7581_pwm 11 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};

	pwmleds-3 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio28_idx12_pins>;

		pwm-0 {
			label = "xPonStatus:green";
			pwms = <&en7581_pwm 12 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};

	pwmleds-4 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio29_idx13_pins>;

		pwm-0 {
			label = "internet:green";
			pwms = <&en7581_pwm 13 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};

	pwmleds-5 {
		compatible = "pwm-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_gpio30_idx14_pins>;

		pwm-0 {
			label = "voip_hook:green";
			pwms = <&en7581_pwm 14 4000000 0>;
			max-brightness = <255>;
			active-low;
		};
	};
};

&en7581_pinctrl {
	gpio-ranges = <&en7581_pinctrl 0 13 47>;

	mdio_pins: mdio-pins {
		mux {
			function = "mdio";
			groups = "mdio";
		};

		conf {
			pins = "gpio2";
			output-high;
		};
	};

	pcie0_rst_pins: pcie0-rst-pins {
		conf {
			pins = "pcie_reset0";
			drive-open-drain = <1>;
		};
	};

	pcie1_rst_pins: pcie1-rst-pins {
		conf {
			pins = "pcie_reset1";
			drive-open-drain = <1>;
		};
	};

	pwm_gpio16_idx8_pins: pwm-gpio16-idx8-pins {
		function = "pwm";
		pins = "gpio16";
		output-enable;
	};

	pwm_gpio25_idx9_pins: pwm-gpio25-idx9-pins {
		function = "pwm";
		pins = "gpio25";
		output-enable;
	};

	pwm_gpio27_idx11_pins: pwm-gpio27-idx11-pins {
		function = "pwm";
		pins = "gpio27";
		output-enable;
	};

	pwm_gpio28_idx12_pins: pwm-gpio28-idx12-pins {
		function = "pwm";
		pins = "gpio28";
		output-enable;
	};

	pwm_gpio29_idx13_pins: pwm-gpio29-idx13-pins {
		function = "pwm";
		pins = "gpio29";
		output-enable;
	};

	pwm_gpio30_idx14_pins: pwm-gpio30-idx14-pins {
		function = "pwm";
		pins = "gpio30";
		output-enable;
	};

	gswp1_led0_pins: gswp1-led0-pins {
		mux {
			function = "phy1_led1";
			pins = "gpio43";
		};
	};

	gswp2_led0_pins: gswp2-led0-pins {
		mux {
			function = "phy2_led1";
			pins = "gpio44";
		};
	};

	gswp3_led0_pins: gswp3-led0-pins {
		mux {
			function = "phy3_led1";
			pins = "gpio45";
		};
	};

	gswp4_led0_pins: gswp4-led0-pins {
		mux {
			function = "phy4_led1";
			pins = "gpio46";
		};
	};

	mmc_pins: mmc-pins {
		mux {
			function = "emmc";
			groups = "emmc";
		};
	};
};

&mmc0 {
	pinctrl-names = "default", "state_uhs";
	pinctrl-0 = <&mmc_pins>;
	pinctrl-1 = <&mmc_pins>;
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	card@0 {
		compatible = "mmc-card";
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			bootloader@0 {
				label = "bootloader";
				reg = <0x00000000 0x00080000>;
			};

			tclinux@80000 {
				label = "tclinux";
				reg = <0x00080000 0x02800000>;
			};

			tclinux_slave@2880000 {
				label = "tclinux_slave";
				reg = <0x02880000 0x02800000>;
			};

			rootfs_data@5080000 {
				label = "rootfs_data";
				reg = <0x5080000 0x00800000>;
			};
		};
	};
};

&i2c0 {
	status = "okay";
};

&pcie0 {
	pinctrl-names = "default";
	pinctrl-0 = <&pcie0_rst_pins>;
	status = "okay";

	pcie@0,0 {
		reg = <0x0000 0 0 0 0>;
		wifi@0,0 {
			compatible = "mediatek,mt76";
			reg = <0x0000 0 0 0 0>;
			airoha,npu = <&npu>;
			airoha,eth = <&eth>;
		};
	};
};

&pcie1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pcie1_rst_pins>;
	status = "okay";

	pcie@1,0 {
		reg = <0x0000 0 0 0 0>;
		wifi@1,0 {
			compatible = "mediatek,mt76";
			reg = <0x0000 0 0 0 0>;
			airoha,npu = <&npu>;
			airoha,eth = <&eth>;
		};
	};
};

&npu {
	status = "okay";
};

&eth {
	status = "okay";
};

&gdm1 {
	status = "okay";
};

&mdio {
	as21xx_0: ethernet-phy@1c {
		reg = <0x1c>;

		firmware-name = "as21x1x_fw.bin";

		reset-deassert-us = <10000>;
		reset-gpios = <&en7581_pinctrl 30 GPIO_ACTIVE_LOW>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@0 {
				reg = <0>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
				function-enumerator = <0>;
				default-state = "keep";
			};

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
				function-enumerator = <1>;
				default-state = "keep";
			};
		};
	};

	as21xx_1: ethernet-phy@1d {
		reg = <0x1d>;

		firmware-name = "as21x1x_fw.bin";

		reset-deassert-us = <10000>;
		reset-gpios = <&en7581_pinctrl 31 GPIO_ACTIVE_LOW>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@0 {
				reg = <0>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
				function-enumerator = <0>;
				default-state = "keep";
			};

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
				function-enumerator = <1>;
				default-state = "keep";
			};
		};
	};
};

&gdm4 {
	status = "okay";

	phy-handle = <&as21xx_1>;
	phy-mode = "usxgmii";
};

&switch {
	pinctrl-names = "default";
	pinctrl-0 = <&mdio_pins>;
	status = "okay";
};

&gsw_phy1 {
	pinctrl-names = "gbe-led";
	pinctrl-0 = <&gswp1_led0_pins>;
	status = "okay";
};

&gsw_phy1_led0 {
	status = "okay";
	active-low;
};

&gsw_phy2 {
	pinctrl-names = "gbe-led";
	pinctrl-0 = <&gswp2_led0_pins>;
	status = "okay";
};

&gsw_phy2_led0 {
	status = "okay";
	active-low;
};

&gsw_phy3 {
	pinctrl-names = "gbe-led";
	pinctrl-0 = <&gswp3_led0_pins>;
	status = "okay";
};

&gsw_phy3_led0 {
	status = "okay";
	active-low;
};

&gsw_phy4 {
	pinctrl-names = "gbe-led";
	pinctrl-0 = <&gswp4_led0_pins>;
	status = "okay";
};

&gsw_phy4_led0 {
	status = "okay";
	active-low;
};
